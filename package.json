{
  "name": "accessible-name-automation-proof-of-concept",
  "description": "Accessible Name Automation (Proof of Concept)",
  "license": "UNLICENSED",
  "author": "Jonathan Linat <jonathan.linat@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:jonathanlinat/accessible-name-automation-proof-of-concept.git"
  },
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "webpack --progress --color",
    "start:dev": "npm run subscript:webpack-dev-server --open",
    "start:ci": "npm run subscript:webpack-dev-server --no-info",
    "test:a11y:violations": "npm run subscript:start-server-and-test subscript:pa11y-ci",
    "test:a11y:accessiblename": "npm run subscript:start-server-and-test subscript:jest",
    "test:a11y:accessiblename:watch": "npm run subscript:start-server-and-test subscript:jest:watch",
    "subscript:jest": "jest",
    "subscript:jest:watch": "jest --watch",
    "subscript:pa11y-ci": "pa11y-ci -c configuration/pa11y-ci/config.json",
    "subscript:start-server-and-test": "start-server-and-test start:ci http-get://localhost:8080",
    "subscript:webpack-dev-server": "webpack-dev-server --color"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "babel-loader": "8.1.0",
    "concurrently": "5.3.0",
    "eslint": "7.9.0",
    "eslint-config-standard": "14.1.1",
    "eslint-config-standard-react": "9.2.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-standard": "4.0.1",
    "html-loader": "1.3.1",
    "html-webpack-plugin": "4.5.0",
    "jest": "26.4.2",
    "jest-puppeteer": "4.4.0",
    "pa11y-ci": "2.4.0",
    "puppeteer": "5.3.0",
    "start-server-and-test": "1.11.4",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-syntax-highlighter": "13.5.3",
    "styled-components": "5.2.0",
    "styled-reset": "4.2.3"
  },
  "jest": {
    "preset": "jest-puppeteer"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-react"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
